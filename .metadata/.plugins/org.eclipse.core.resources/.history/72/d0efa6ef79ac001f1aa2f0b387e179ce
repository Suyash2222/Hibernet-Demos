package com.mydata;

import java.io.Serializable;

import org.omg.CORBA.PUBLIC_MEMBER;

public class Product implements Serializable {
	
	private int pid;
	private String salary;
	private float cost;
	private int qty;
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getSalary() {
		return salary;
	}
	public void setSalary(String salary) {
		this.salary = salary;
	}
	public float getCost() {
		return cost;
	}
	public void setCost(float cost) {
		this.cost = cost;
	}
	public int getQty() {
		return qty;
	}
	public void setQty(int qty) {
		this.qty = qty;
	}
	
	public Product() {
		// TODO Auto-generated constructor stub
	}
		
	
	public Product(int pid, String salary, float cost, int qty) {
		super();
		this.pid = pid;
		this.salary = salary;
		this.cost = cost;
		this.qty = qty;
	}
	@Override
	public String toString() {
		return "Product [pid=" + pid + ", salary=" + salary + ", cost=" + cost + ", qty=" + qty + "]";
	}
	

	
	
	
}
